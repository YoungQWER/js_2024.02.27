/**
 * 
 */

 function check(){
	 if(document.frm.id.vaule == ""){
		 alert("아이디를 입력해 주세요!")
		 document.frm.id.foucs();
		 return false;
	 }else if(document.frm.pass.vaule == ""){
		 alert("비밀번호를 입력해 주세요!")
		 document.frm.pass.foucs();
		 return false;
	 }else if(isNaN(document.frm.pass.value)){
		 alert("정수만 입력해 주세요!")
		 document.frm.pass.foucs();
		 return false;
	 }
 }